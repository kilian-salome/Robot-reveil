void remettreHeure(struct ts t, int heure, int minu, int sec, int mday, int mon, int year) {
  t.hour = heure;
  t.min = minu;
  t.sec = sec;
  t.mday = mday;
  t.mon = mon;
  t.year = year;

  DS3231_set(t);
}

void afficherDateMONI(struct ts t) {
  Serial.print("date : ");
  Serial.print(t.mday);
  Serial.print("/");
  Serial.print(t.mon);
  Serial.print("/");
  Serial.print(t.year);
  Serial.print("\t Heure : ");
  Serial.print(t.hour);
  Serial.print(":");
  Serial.print(t.min);
  Serial.print(".");
  Serial.println(t.sec);
}

